<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   
   <extension
        point="org.eclipse.emf.ecp.view.context.viewServices">
     <viewService
           class="org.eclipse.emf.ecp.ecoreeditor.ecore.referenceservices.EcoreReferenceService">
     </viewService>
   </extension>
   
 	<extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.DataTypeControl">
         <dynamicTest
               testClass="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.DatatypeRendererTest">
         </dynamicTest>
      </renderer>
   </extension>
   
    	<extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.TypedElementBoundsRenderer">
         <dynamicTest
               testClass="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.TypedElementBoundsRendererTester">
         </dynamicTest>
      </renderer>
   </extension>
   
   <extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.AttributesView">
         <dynamicTest
               testClass="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.AttributesViewTest">
         </dynamicTest>
      </renderer>
   </extension>
   
   <extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.EEnumLiteralRenderer">
			<dynamicTest
               testClass="org.eclipse.emf.ecp.ecoreeditor.ecore.controls.EEnumLiteralRendererTest">
         </dynamicTest>
      </renderer>
   </extension>


