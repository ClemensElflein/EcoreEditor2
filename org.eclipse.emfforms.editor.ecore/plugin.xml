<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
      
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.emfforms.spi.editor.GenericEditor"
            default="true"
            extensions="ecore"
            icon="icons/EcoreModelFile.gif"
            id="EcoreEditor"
            name="Ecore Editor">
      </editor>
   </extension>
   <extension
        point="org.eclipse.emf.ecp.view.context.viewServices">
     <viewService
           class="org.eclipse.emfforms.internal.editor.ecore.referenceservices.EcoreReferenceService">
     </viewService>
   </extension>
   
 	<extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emfforms.internal.editor.ecore.controls.DataTypeControl">
         <dynamicTest
               testClass="org.eclipse.emfforms.internal.editor.ecore.controls.DatatypeRendererTest">
         </dynamicTest>
      </renderer>
   </extension>
   
    	<extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emfforms.internal.editor.ecore.controls.TypedElementBoundsRenderer">
         <dynamicTest
               testClass="org.eclipse.emfforms.internal.editor.ecore.controls.TypedElementBoundsRendererTester">
         </dynamicTest>
      </renderer>
   </extension>
   
   <extension
         point="org.eclipse.emf.ecp.ui.view.swt.renderers">
      <renderer
            renderer="org.eclipse.emfforms.internal.editor.ecore.controls.EEnumLiteralRenderer">
			<dynamicTest
               testClass="org.eclipse.emfforms.internal.editor.ecore.controls.EEnumLiteralRendererTest">
         </dynamicTest>
      </renderer>
   </extension>


<extension  point="org.eclipse.emf.ecp.view.model.provider.xmi.file">
<file filePath="viewmodels/EParameter.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EEnum.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EDataType.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EReference.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EAttribute.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EClass.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>

<file filePath="viewmodels/EPackage.view">
	<filter
       key="detail"
       value="true">
	</filter>
</file>


<file filePath="simple_viewmodels/EParameter.view"/>

<file filePath="simple_viewmodels/EEnum.view"/>

<file filePath="simple_viewmodels/EDataType.view"/>

<file filePath="simple_viewmodels/EReference.view"/>

<file filePath="simple_viewmodels/EAttribute.view"/>

<file filePath="simple_viewmodels/EClass.view"/>

<file filePath="simple_viewmodels/EPackage.view"/>

</extension>
</plugin>
